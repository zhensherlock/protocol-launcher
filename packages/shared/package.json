{
  "name": "@protocol-launcher/shared",
  "version": "1.0.0",
  "description": "One-click launch URL generator for protocol-based apps.",
  "type": "module",
  "author": "zhensherlock",
  "homepage": "https://zhensherlock.github.io/protocol-launcher",
  "keywords": [
    "protocol-launcher",
    "quick-launcher",
    "deep-link",
    "deeplink",
    "url-scheme",
    "url-generator",
    "launch-url",
    "protocol",
    "mcp",
    "modelcontextprotocol",
    "mcp-installer",
    "cursor",
    "cherry-studio",
    "llm-provider",
    "api-configuration",
    "ai-tools",
    "one-click",
    "tree-shaking",
    "esm",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhensherlock/protocol-launcher.git"
  },
  "scripts": {
    "clean:dist": "rimraf dist",
    "dev": "rolldown -c rolldown.config.ts --watch --no-clear-screen",
    "build:types": "tsc",
    "build": "npm run clean:dist && npm run build:types && rolldown -c rolldown.config.ts"
  },
  "bugs": {
    "url": "https://github.com/zhensherlock/protocol-launcher/issues"
  },
  "dependencies": {}
}
