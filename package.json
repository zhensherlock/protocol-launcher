{
  "name": "protocol-launcher",
  "version": "0.0.0",
  "description": "One-click launch URL generator for protocol-based apps.",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "cross-env NODE_ENV=development turbo run dev --filter=./apps/docs...",
    "build": "cross-env NODE_ENV=production turbo run build --filter=./apps/docs...",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "changeset": "changeset",
    "version": "changeset version"
  },
  "author": "zhensherlock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhensherlock/protocol-launcher/issues"
  },
  "homepage": "https://zhensherlock.github.io/protocol-launcher",
  "keywords": [
    "protocol-launcher",
    "quick-launcher",
    "deep-link",
    "deeplink",
    "url-scheme",
    "url-generator",
    "launch-url",
    "protocol",
    "mcp",
    "modelcontextprotocol",
    "mcp-installer",
    "cursor",
    "cherry-studio",
    "llm-provider",
    "api-configuration",
    "ai-tools",
    "one-click",
    "tree-shaking",
    "esm",
    "typescript"
  ],
  "packageManager": "pnpm@10.28.0",
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "22",
    "@vitest/coverage-v8": "^4.0.16",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "rolldown": "^1.0.0-beta.59",
    "turbo": "^2.7.3",
    "typescript": "^5.9.3",
